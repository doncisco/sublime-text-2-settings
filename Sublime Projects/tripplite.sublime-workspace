:::::::::::::::::::::::\n			local.requiredFieldsFilled = super.processForm(errorLabels = variables.rbForm.requiredFieldErrors); \n			\n			// Required fields test fail. formFields.errorMessages is updated by parent object. Simply exit\n			if( !local.requiredFieldsFilled ){ return false; }	\n\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n		// Create user bean from info passed in - this will trigger data validation\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n			local.createUserBean = variables.userService.create(\n				countryCode    = this.formFields.countryCode.value,\n				firstName      = this.formFields.firstName.value,\n				familyName     = this.formFields.familyName.value,\n				emailAddress   = this.formFields.emailAddress.value,\n				title          = this.formFields.title.value,\n				companyName    = this.formFields.companyName.value,\n				userStatus	   = \"visitor\"\n			);\n\n			// Found errors with user bean data. Update error messages and exit.\n			if( !local.createUserBean.success ){\n				// Loop through errors structure and determine which form fields to update with language specific error messages\n				for(local.key in local.createUserBean.errors) {\n					if( structKeyExists(this.formFields, local.key) ){\n						// Using the form field ids (which match the bean properties) as the keys for the resource bundle.\n						this.formFields[local.key].errorMessage &= variables.rbForm.fieldDataErrors[local.key] & \"<br>\";\n					}else{\n						// No matching form element to insert error into, show in form-wide errors\n						variables.formErrorMessage &= variables.rbForm.fieldDataErrors[local.key] & \"<br>\";\n					}\n				}\n				\n				return false;\n			}else{\n				// Check for existing user by email address\n				local.findUserByEmail = variables.userService.getByAttributes( emailAddress = this.formFields.emailAddress.value );\n\n				// Set DB user or new user bean to signup for newsletter\n				local.userBean = local.findUserByEmail.success ? local.findUserByEmail.data[1] : local.createUserBean.data;\n			}\n\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n		// Create company related records from info passed in\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n			if( len(this.formFields.companyName.value) ){\n				// Create company bean from info passed in - this will trigger data validation\n				local.createCompanyBean = variables.companyService.create(\n					companyTypeID = 0,\n					companyName = this.formFields.companyName.value\n				);\n\n				// Found errors with company bean data. Update error messages and exit.\n				if( !local.createCompanyBean.success ){\n					// Loop through errors structure and determine which form fields to update with language specific error messages\n					for( local.key in local.createCompanyBean.errors ){\n						if( structKeyExists(this.formFields, local.key) ){\n							// Using the form field ids (which match the bean properties) as the keys for the resource bundle.\n							this.formFields[local.key].errorMessage &= variables.rbForm.fieldDataErrors[local.key] & \"<br>\";\n						}else{\n							// No matching form element to insert error into, show in form-wide errors\n							variables.formErrorMessage &= variables.rbForm.fieldDataErrors[local.key] & \"<br>\";\n						}\n					}\n					return false;\n				}else{\n					// Always create new company and update web_user record. \n					// Don't use existing company records that may be found in existing user's Company_ID;\n					local.companyBean = local.createCompanyBean.data;\n					\n					// Save new company data\n					local.saveCompany = variables.companyService.save( local.companyBean );\n\n					// Ignore Problems saving company bean information to DB\n					if( local.saveCompany.success ){\n						// Add company id to (new/existing user) without an associated company record\n						local.userBean.setCompanyID( local.companyBean.getCompanyID() );\n\n						// Industries Selected? Make company industry DB entries\n						if( len(this.formFields.industry.value) ){\n							// Industry checkbox selections will be list\n							for (i=1; i<=listLen(this.formFields.industry.value); i=i+1) {\n								variables.companyService.saveCompanyIndustry(\n									companyID = local.companyBean.getCompanyID(),\n									industryID = listGetAt(this.formFields.industry.value, i)\n								);\n							}\n						}\n					}\n				}\n			}\n\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n		// Update user bean and save to DB\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n			// Update lead source for all users - dan fiedler appends this info in SF DB when it changes in webcat\n			local.userBean.setLeadSource( \"NEWSLETTER SUBSCRIPTION\" );\n\n			// User wishes to recieve emails\n			local.userBean.setDoNotEmail( false ); \n\n			// Saves new user OR updates found user (and their doNotEmail status)\n			local.saveUser = variables.userService.save( local.userBean );\n\n			// Problems saving user bean information to DB\n			if( !local.saveUser.success ){ \n				variables.formErrorMessage = variables.rbPage.newsletterSignup.fail;\n				return false; \n			}\n\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n		// Signup user for tripplite newsletter\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n			local.userType = this.formFields.isReseller.checked ? 2 : 1; \n\n			// Create user subscription bean. initiates data validation\n			local.createUserSubscriptionBean = variables.userSubscriptionService.create(\n				userID            = local.userBean.getUserID(),\n				subscriptionID    = local.userType,\n				startDate         = NOW(),\n				subscriptionEmail = local.userBean.getEmailAddress()\n			);\n\n			// Data good. Save user subscription to DB\n			if( local.createUserSubscriptionBean.success ){\n				variables.userSubscriptionService.save( local.createUserSubscriptionBean.data );\n			}else{	\n				variables.formErrorMessage = variables.rbPage.newsletterSignup.fail & \"<br>\";\n				return false;\n			}\n\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n		// Log user Activity?\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n		\n\n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n		// We got this far means processing form was successful. \n		// ::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::\n			return true;\n		</cfscript>\n	</cffunction>\n</cfcomponent>",
