escription\n				this.formFields.otherIndustryDescription.value = arguments.dataStruct.otherIndustryDescription; \n\n				//Add other value to the list of selected industries \n				this.formFields.industry.value = listAppend(this.formFields.industry.value, this.formFields.otherIndustry.value);\n			}\n		</cfscript>\n	</cffunction>\n\n	<cffunction name=\"generateForm\" access=\"public\" output=\"false\" returntype=\"void\" hint=\"I generate a newsletter signup form.\">\n		<cfprocessingdirective suppressWhitespace=\"true\">\n			<cfsavecontent variable=\"this.formHTML\">\n				<cfoutput>\n					<form class=\"form-horizontal col-md-12 well\" action=\"#variables.actionPage#\" method=\"post\" name=\"newsletterSingupForm\" id=\"newsletterSingupForm\">\n						<!--- Display form-wide error messages --->\n						<cfif variables.formErrorMessage NEQ \"\">\n							<div class=\"alert alert-danger\">\n								<button type=\"button\" class=\"close\" data-dismiss=\"alert\">&times;</button>\n								#variables.formErrorMessage#\n							</div>\n						</cfif>\n						\n						<cfif variables.formSuccessMessage NEQ \"\">\n							<div class=\"alert alert-success\">\n								<button type=\"button\" class=\"close\" data-dismiss=\"alert\">&times;</button>\n								#variables.formSuccessMessage#\n							</div>\n						</cfif>\n\n						<!--- main fields --->\n						<div class=\"row padLeft20\">\n							<div class=\"col-md-3\">\n								#createSelectList( argumentCollection = this.formFields.countryCode )#\n							</div>\n						</div>\n						<div class=\"row padLeft20\">\n							<div class=\"col-md-3\">\n								#createInputBox( argumentCollection = this.formFields.firstName )#\n							</div>\n							<div class=\"col-md-3 col-md-offset-1\">\n								#createInputBox( argumentCollection = this.formFields.familyName )#\n							</div>\n						</div>\n						<div class=\"row padLeft20\">\n							<div class=\"col-md-7\">\n								#createInputBox( argumentCollection = this.formFields.emailAddress )#\n							</